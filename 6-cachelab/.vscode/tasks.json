{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc 生成活动文件",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "-Wall",
                "-Werror",
                "-std=c99",
                "-m64",
                "-o",
                "csim",
                "csim.c",
                "cachelab.c",
                "-lm",
                /*"-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
                */
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}